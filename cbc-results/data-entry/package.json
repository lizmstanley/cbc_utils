{
  "name": "cbc_results",
  "version": "1.0.0",
  "description": "Utilities for entering CBC results data into the CBC database.",
  "author": "Liz Stanley <lizmstanley@gmail.com>",
  "repository": "github:lizmstanley/cbc-utils",
  "license": "GPL-3.0-or-later",
  "type": "module",
    "engines": {
    "node": ">=25.0.0"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "load-db": "tsx util/load-db.ts --force",
    "start-audubon": "tsx audubon/audubon-data-entry.ts",
    "start-mou": "tsx mou/mou-data-entry.ts",
    "format": "prettier . --write",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "csvtojson": "^2.0.14",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "puppeteer": "^24.34.0",
    "better-sqlite3": "^12.6.0",
    "html-entities": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.47.0",
    "@types/node": "^25.0.3",
    "@types/better-sqlite3": "^7.4.2",
    "@types/yargs": "^17.0.35",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "yargs": "^18.0.0"
  }
}
